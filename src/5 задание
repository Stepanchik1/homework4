public class Main {
    public static void main(String[] args) {
        int age = 25;
        if (age<=6) {
            System.out.println("Ребенок собирается в школу");}
        else {
            System.out.println("Ребенок пошел в школу");}
        if (age>=18) {
            System.out.println("Ребенок закончил школу");}
        else {
            System.out.println("Ребенок не закончил школу");}
        if (age>=24) {
            System.out.println("Человек ищет работу");}
        else {
            System.out.println("Пока не нуждается в работе");
        }
    }}