public class Main {
    public static void main(String[] args) {
        int A = 60;
        if (A<120){
            System.out.println("Есть места");
        }
        else {
            System.out.println("Местов нет");
        }
        if (A>=60&&A<120){
            System.out.println("Есть стоячие места");}
        if (A<60){
            System.out.println("Есть сидячие места");}
    }}